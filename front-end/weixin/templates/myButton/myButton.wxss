.tabBarButton-view {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tabBarButton-icon {
  height: 100rpx;
  width: 100rpx;
}

.tabBarButton-hover {
  opacity: 0.6;
}

.tabBarButton-text {
  margin-top: 8rpx;
  font-size: 26rpx;
}

.lineButton-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 88rpx;
  width: 100%;
  background-color: #fafdff;
}

.lineButton-img {
  height: 60rpx;
  width: 60rpx;
  margin-left: 44rpx;
}

.lineButton-text {
  margin-left: 50rpx;
  font-size: 36rpx;
}

.lineButton-hover {
  background-color: #dfdfdf;
}

.lineButton-symbol{
  height: 25rpx;
  width: 16rpx;
  margin-left: 400rpx;
}

.coolButton-view{
  height: 88rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 36rpx;
  color: white;
  border-radius: 44rpx;
}

.slimcoolButton-view{
  height: 66rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 36rpx;
  color: white;
  border-radius: 33rpx;
}