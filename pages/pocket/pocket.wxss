/* pages/pocket/pocket.wxss */

.background {
  position: absolute;
  z-index: 0;
  background-color: #6193cd;
}

.scroll {
  position: absolute;
  z-index: 1;
  background-color: #f0f3f5;
  border-radius: 60rpx 60rpx 0 0;
}

.scrollView{
    border-radius: 60rpx 60rpx 0 0;
    display: flex;
    flex-direction: column;
    align-items: center;
}