.tabBarButton-view {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tabBarButton-icon {
  height: 100rpx;
  width: 100rpx;
}

.tabBarButton-hover {
  opacity: 0.6;
}

.tabBarButton-text {
  margin-top: 8rpx;
  font-size: 26rpx;
}

.lineButton-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 88rpx;
  width: 100%;
  background-color: #fafdff;
}

.lineButton-img {
  height: 60rpx;
  width: 60rpx;
  margin-left: 44rpx;
}

.lineButton-text {
  margin-left: 50rpx;
  font-size: 36rpx;
}

.lineButton-hover {
  background-color: #fafafa;
}

.lineButton-symbol{
  position: absolute;
  right: 30rpx;
  color: gray;
}

.coolButton-view{
  height: 92rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: white;
  border-radius: 46rpx;
}