/* pages/newresult/newresult.wxss */
@import "../../templates/myButton/myButton.wxss";
.pageView {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  background-color:#f0f3f5;
}

.background {
  position: absolute;
  z-index: 0;
  top: 48rpx;
  width: 90%;
  height: 600rpx;
}

.commonItem {
  position: absolute;
  z-index: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.line {
  width: 90%;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.logoLine {
  width: 90%;
  margin-top: 76rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.logo {
  height: 30rpx;
  width: 50rpx;
}

.title {
  font-size: 24rpx;
  margin-left: 8rpx;
}

.invoiceHead {
  font-size: 35rpx;
  color: #973737;
  justify-content: center;
  margin-top: 105rpx;
}

.topic {
  margin-left: 50rpx;
  color: #b1b1b1b1;
  font-size: 30rpx;
}

.item {
  margin-right: 50rpx;
  font-size: 30rpx;
}

.xian {
  width: 81%;
  height: 5rpx;
  margin-top: 20rpx;
}

#attrs {
  margin-top: 30rpx;
  width: 82%;
  align-self: center;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 26rpx;
  color: #b1b1b1b1;
}

.swiper {
  height: 215rpx;
  width: 82%;
  align-self: center;
  font-size: 20rpx;
}

.swiper-item {
  display: flex;
  flex-direction: row;
  margin-top: 13rpx;
}

.moreinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}